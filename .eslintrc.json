{
  "plugins": ["unused-imports", "@typescript-eslint"],
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:@typescript-eslint/recommended",
    "next"
  ],
  "parser": "@typescript-eslint/parser",
  "root": true,
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "unused-imports/no-unused-imports": "error",
    "no-var": "error"
  }
}
